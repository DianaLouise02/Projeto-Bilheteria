</head>
<body>
    <%- include('partials/header') %>
    <div class="container">
        <h1>Junte-se a nós</h1>
        <form action="#">
            <label for="nome">Nome da empresa:</label>
            <input type="text" id="nome" name="nome" required>
 
            <label for="email">Email:</label>
            <input type="email" id="email" name="email" required>
 
            <label for="numero_documento">CNPJ da empresa:</label>
            <input type="number" id="numero_documento" name="numero_documento" required max="14" min="14">
 
            <label for="sexo">Categoria de Ingressos:</label>
 
            <label for="codigo_pais">Código do País (para o número de telefone):</label>
            <input type="number" id="codigo_pais" name="codigo_pais" required>
 
            <label for="numero_telefone">Número de Telefone:</label>
            <input type="number" id="numero_telefone" name="numero_telefone" required max="11"  min="9">
 
           
 
            <button class="submit-btn" type="submit">Enviar</button>
        </form>
    </div>
    <%- include('partials/footer') %>
</body>
</html>
 
